{
  "homepage": "http://.",
  "name": "react_sum",
  "version": "0.1.0",
  "private": true,
  "keywords": [],
  "author": "Mate Academy",
  "license": "GPL-3.0",
  "dependencies": {
    "node-sass": "^4.14.1",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-scripts": "3.4.1"
  },
  "devDependencies": {
    "@mate-academy/eslint-config-react": "*",
    "@mate-academy/stylelint-config": "*",
    "cypress": "^5.6.0",
    "eslint": "^6.8.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.1.0",
    "gh-pages": "^2.2.0",
    "husky": "^4.0.10",
    "lint-staged": "^9.5.0",
    "stylelint": "^13.0.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint-js": "eslint --ext .jsx,.js ./src/ --fix",
    "lint-css": "stylelint src/**/*.css src/**/*.scss --fix",
    "lint": "npm run lint-js && npm run lint-css",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "lint-staged": {
    "*.css": [
      "stylelint",
      "git add"
    ],
    "*.js": [
      "eslint",
      "git add"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "description": "- Replace `<your_account>` with your Github username in the [DEMO LINK](https://<your_account>.github.io/react_sum/) - Follow the [React task guideline](https://github.com/mate-academy/react_task-guideline#react-tasks-guideline)",
  "main": ".eslintrc.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/iBrianWarner/react_sum.git"
  },
  "bugs": {
    "url": "https://github.com/iBrianWarner/react_sum/issues"
  }
}
